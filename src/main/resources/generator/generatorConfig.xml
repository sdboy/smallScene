<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
    "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
  <!--导入属性配置-->
  <properties resource="datasource.properties" />
  <classPathEntry location="${jdbc.driverLocation.mysql}"/>

  <context id="context" targetRuntime="MyBatis3">
    <commentGenerator>
      <property name="suppressAllComments" value="true"/>
      <property name="suppressDate" value="true"/>
    </commentGenerator>

    <jdbcConnection userId="${jdbc.user3}" password="${jdbc.password3}" driverClass="${jdbc.driverClassName.mysql}"
                    connectionURL="${jdbc.url3}"/>

    <javaTypeResolver>
      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.kelan.smallscene.entity" targetProject="./src/main/java">
      <property name="enableSubPackages" value="false"/>
      <property name="constructorBased" value="true"/>
      <property name="trimStrings" value="true"/>
      <property name="immutable" value="false"/>
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mapper" targetProject="./src/main/resources">
      <property name="enableSubPackages" value="false"/>
    </sqlMapGenerator>

    <javaClientGenerator targetPackage="com.kelan.smallscene.dao" type="XMLMAPPER" targetProject="./src/main/java">
      <property name="enableSubPackages" value="false"/>
    </javaClientGenerator>

    <table tableName="MAN_REAL_POSITION" domainObjectName="ManRealPositionDto" enableCountByExample="false"
           enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
           selectByExampleQueryId="false">
      <columnOverride column="LATITUDE" jdbcType="DOUBLE" javaType="java.lang.Double" />
      <columnOverride column="LONGITUDE" jdbcType="DOUBLE" javaType="java.lang.Double" />
    </table>

  </context>
</generatorConfiguration>